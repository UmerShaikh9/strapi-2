{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Picture": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Summary": {
      "type": "text",
      "required": false
    },
    "Description": {
      "type": "text"
    },
    "Size": {
      "type": "customField",
      "default": "[]",
      "options": [
        "S",
        "M",
        "L",
        "XL",
        "FREE SIZE"
      ],
      "required": true,
      "customField": "plugin::multi-select.multi-select"
    },
    "Quantity": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "Color": {
      "type": "customField",
      "default": "[]",
      "options": [
        "WHITE",
        "BLACK",
        "RED",
        "GREEN",
        "BLUE",
        "ORANGE",
        "BROWN",
        "PURPLE",
        "YELLOW",
        "MAROON",
        "PINK",
        "GOLD",
        "TEAL",
        "SILVER",
        "GREY",
        "MIDNIGHT",
        "WINE",
        "BEIGE",
        "RUST",
        "PEACH",
        "TUSCAN"
      ],
      "required": true,
      "customField": "plugin::multi-select.multi-select"
    },
    "Fabric": {
      "type": "customField",
      "default": "[]",
      "options": [
        "option1",
        "option2"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "Price_Section": {
      "type": "component",
      "repeatable": true,
      "component": "user-product.prize-section"
    },
    "Badges": {
      "type": "customField",
      "default": "[]",
      "options": [
        "New Arrival",
        "Ready To Ship",
        "Popular"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "Product_Details": {
      "type": "component",
      "repeatable": true,
      "component": "product.product-details"
    },
    "Collections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::collection.collection",
      "mappedBy": "Products"
    },
    "Categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "Products"
    }
  }
}
